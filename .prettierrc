{
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 150,
  "overrides": [{
      "files": ".prettierrc",
      "options": {
          "parser": "json"
      }
  }],
  "editor.formatOnSave": true,
  "proseWrap": "always",
  "tabWidth": 3,
  "requireConfig": false,
  "useTabs": false,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "semi": true
}